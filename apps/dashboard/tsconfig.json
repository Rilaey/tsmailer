{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "module": "commonjs",
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "public/data/**/*.json", "pages/api/auth/[...nextauth].js"],
  "exclude": [
    "node_modules"
  ]
}
